components:
    vivarium_public_health:
        population:
            - BasePopulation()
            - Mortality()
        #disease.models:
        #    - SIS('shigellosis')
        metrics:
            - DisabilityObserver()
            - MortalityObserver()
    vivarium_gates_shigella_vaccine.components:
        - FertilityCrudeBirthRate()

configuration:
    input_data:
        location: {{ location_proper }}
        input_draw_number: 0
        artifact_path: /share/costeffectiveness/artifacts/vivarium_gates_shigella_vaccine/{{ location_sanitized }}.hdf
    interpolation:
        order: 0
    randomness:
        map_size: 1_000_000
        key_columns: ['entrance_time', 'age']
        random_seed: 0
    time:
        start:
            year: 2025
            month: 7
            day: 2
        end:
            year: 2040
            month: 7
            day: 2
        step_size: 1 # Days
    population:
        population_size: 8000
        age_start: 0
        age_end: 5
        exit_age: 5
    shigellosis_vaccine:
        scenario: nine_twelve_minimal
    metrics:
        shigellosis_vaccine_observer:
                by_age: True
                by_sex: True
                by_year: True
        shigellosis_observer:
                by_age: True
                by_sex: True
                by_year: True
        mortality:
                by_age: True
                by_sex: True
                by_year: True
        disability:
                by_age: True
                by_sex: True
                by_year: True
        yll_forecast_observer:
                by_age: True
                by_sex: True
                by_year: True
